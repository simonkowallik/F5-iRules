when CLIENTSSL_HANDSHAKE {
	log local0.debug "[IP::client_addr]:[TCP::client_port]:> RSA Session-ID:[SSL::sessionid] Master-Key:[SSL::sessionsecret]"
}
when SERVERSSL_HANDSHAKE {
	log local0.debug "[IP::server_addr]:[TCP::server_port]:> RSA Session-ID:[SSL::sessionid] Master-Key:[SSL::sessionsecret]"
}
